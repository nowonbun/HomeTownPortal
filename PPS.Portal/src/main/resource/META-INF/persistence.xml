<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="portal">
		<class>model.Card</class>
		<class>model.CardStep</class>
		<class>model.CardType</class>
		<class>model.Cookie</class>
		<class>model.CookiePK</class>
		<class>model.Password</class>
		<class>model.PasswordPK</class>
		<class>model.State</class>
		<class>model.StateInfo</class>
		<class>model.User</class>
		<class>model.Comment</class>
		<class>model.Role</class>
		<class>model.Company</class>
		<class>model.Group</class>
		<class>model.LookUp</class>
		<class>map.MapViewRoleCompany</class>
		<class>map.MapViewRoleCompanyPK</class>
		<class>map.MapViewRoleGroup</class>
		<class>map.MapViewRoleGroupPK</class>
		<class>map.MapViewRoleUser</class>
		<class>map.MapViewRoleUserPK</class>
		<class>map.MapActionRoleCompany</class>
		<class>map.MapActionRoleCompanyPK</class>
		<class>map.MapActionRoleGroup</class>
		<class>map.MapActionRoleGroupPK</class>
		<class>map.MapActionRoleUser</class>
		<class>map.MapActionRoleUserPK</class>
		<properties>
			<property name="eclipselink.logging.level" value="INFO" />
			<property name="eclipselink.logging.parameters" value="true" />
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver" />
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/portal" />
			<property name="javax.persistence.jdbc.user" value="test" />
			<property name="javax.persistence.jdbc.password" value="a12345" />
		</properties>
	</persistence-unit>
</persistence>
